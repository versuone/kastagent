<config title='OAT Environment Management' temp_dir='/tmp'>


    <menu title='menua' help='My menua help !' lhelp='My menua Long help !!'>
        <option name='optiona1' command='ls -ltr' help='My short help'/>
        <option name='optiona2' command='dir' confirm='True' help='MyHelp optiona2'/>
        <option name='imbmenu' command='$[kwad_home]/bin/menu  $[kwad_home]/samples/apimenu/menu2_follow.xml --follow_menu -v100'/>
        <option name='tplmenu' command='$[kwad_home]/bin/tplmenu --follow_menu -v300' set_color='True' police_bold='True' police_color='BLUE' police_bgcolor='NONE'/>
        <option name='optiona4' command='optiona4' help='MyHelp optiona4'/>
            <menu title='menub' help='My menub help !'  lhelp='My menub Long help !!'>
                <option name='optionb1' command='dir'/>
                <option name='optionb3' command='ls -ltr'/>
                <option name='optionb4' command='dir'/>
                <option name='optionb5' command='ls -ltr'/>
                <menu title='menuc' help='My menuc help !'  lhelp='My menuc Long help !!'>
                    <option name='optionc1' command='ls -ltr'/>

                    <imenu title='MyIMenu1' help='MyIMenu1 help.' command='dir $field1 $field2 $field3' confirm='True'>
                        <ioption name='field1' help='myfield1 Short help !' lhelp='myfield1 Long HELP !!'  wk='{*type:str,*value:test1.dat}'/>
                        <ioption name='field2' help='myfield2 help'>
                            {*type:list,*ltype:{*type:int},*value:[1,2,3]}
                        </ioption>
                        <ioption name='field3' help='myfield3 help' wk='{*type:str,*value:test3.dat}'/>
                        
                        <option name='option1' command='dir'/>
                        <option name='option2' command='ls -ltr' help='MyHelp optiona4'/>
                        <menu title='menub'>
                            <option name='optionb1' command='dir'/>
                            <option name='optionb3' command='ls -ltr'/>
                            <option name='optionb4' command='dir'/>
                            <option name='optionb5' command='ls -ltr'/>
                        </menu>   
                        
                        <imenu title='MyIMenu2' help='MyIMenu2 help.' command='dir $field1 $field2 $field3' confirm='True'>
                            <ioption name='field1' help='myfield1 help' wk='{*value:test1.dat,*label:My Field1}'/>
                            <ioption name='field2' help='myfield2 help'>
                                {*type:list,*value:[test1.dat,test2.dat,test3.dat,a,b,c,d,e,f,g,h,j,k,l,mk,l,m],*label:My Field2}
                            </ioption>
                            <ioption name='field3' help='myfield3 help' wk='{*value:a,*checkIn:(a,b,c)}'/>
                            <ioption name='field4' help='myfield4 help' wk='{*type:int,*value:3,*label:My Field4}'/>
                            <ioption name='field5' help='myfield5 help'>
                                {*type:list}
                            </ioption>
                            <ioption name='field6'>
                                {*type:list,*ltype:{*type:int},*value:[1,2,3]}
                            </ioption>
                        </imenu>
        
                    </imenu>                    
                    
                    <option name='optionc5' command='dir'/>
                </menu>
                <option name='option6' command='ls -ltr'/>
            </menu>
        <option name='option5' command='ls -ltr' help='MyHelp optiona5'/>
        <menu title='menud'>
            <option name='optiond1' command='kupd --kact jss.ssl --console --follow_menu -v100'/>
            <option name='optiond2' command='ls -ltr'/>
            <option name='optiond3' command='dir'/>
            
            <imenu title='MyIMenu2' help='My IMenu2 Short help.' command='dir $field1 $field2 $field3' confirm='True' set_color='True' police_bold='True' police_color='RED' police_bgcolor='YELLOW'>
                <ioption name='field1' help='myfield1 Short help !'  lhelp='myfield1 Long help !!' wk='{*value:test1.dat,*label:My Field1}'/>
                <ioption name='field2' help='myfield2 help'>
                    {*type:list,*value:[test1.dat,test2.dat,test3.dat,a,b,c,d,e,f,g,h,j,k,l,mk,l,m],*label:My Field2}
                </ioption>
                <ioption name='field3' help='myfield3 help' wk='{*value:a,*checkIn:(a,b,c)}'/>
                <ioption name='field4' help='myfield4 help' wk='{*type:int,*value:3,*label:My Field4}' set_color='True' police_bold='True' police_color='CYAN' police_bgcolor='GREEN'/>
                <ioption name='field5' help='myfield5 help'>
                    {*type:list}
                </ioption>
                <ioption name='field6'>
                    {*type:list,*ltype:{*type:int},*value:[1,2,3]}
                </ioption>
            </imenu>
            
            <option name='optiond1' command='ls -ltr'/>
            <option name='optiond2' command='ls -ltr'/>
            <option name='optiond3' command='ls -ltr'/>
        </menu>
        <option name='optiona15' command='ls -ltr'/>
        <option name='optiona26' command='dir'/>
        <option name='optiona35' command='ls -ltr'/>
        <option name='optiona46' command='dir'/>
        <option name='optiona55' command='ls -ltr'/>
        <option name='optiona66' command='dir'/>
        <option name='optiona75' command='ls -ltr'/>
        <option name='optiona86' command='dir'/>
        <option name='optiona95' command='ls -ltr'/>
        <option name='optiona06' command='dir'/>
    </menu>

</config>
